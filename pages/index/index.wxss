swiper{
  width: 100%;
  height: 340rpx;
}
swiper image{
  display: block;
  width: 100%;
  height: 100%;
}

.items-list{

}
.pro-item{
  overflow: hidden;
  padding: 30rpx;
}
.pro-logo{
  width: 190rpx;
  height: 190rpx;
  float: left;
}
.pro-body{
  margin-left: 200rpx;
}
.pro-title{
  color: #212121;
  font-size: 34rpx;
  line-height: 1;
}
.pro-desc{
